{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "main": "backend/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack-dev-server --content-base build/",
    "backend": "nodemon backend/",
    "start": "npm run build && NODE_ENV=production node backend/",
    "sync": "node backend/postgres/sync.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "history": "^4.7.2",
    "node-sass": "^4.6.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.3",
    "sequelize": "^4.35.5",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "webpack-dev-middleware": "^1.10.1",
